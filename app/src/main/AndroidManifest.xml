<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.liu.youcai">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.YouCaiActivity">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->


            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->
        </activity>
        <activity
            android:name=".ui.AddActivity"
            android:parentActivityName=".ui.YouCaiActivity" />
        <activity
            android:name=".ui.UserSettingActivity"
            android:parentActivityName=".ui.YouCaiActivity" />
        <activity android:name=".ui.RegisterActivity" />
        <activity
            android:name=".ui.ChangePasswordActivity"
            android:parentActivityName=".ui.UserSettingActivity" />
        <activity
            android:name=".ui.DetailActivity"
            android:parentActivityName=".ui.YouCaiActivity" />
        <activity
            android:name=".ui.StatisticsActivity"
            android:parentActivityName=".ui.YouCaiActivity" />
        <activity android:name=".ui.AboutActivity"
            android:parentActivityName=".ui.YouCaiActivity"></activity>
    </application>

</manifest>